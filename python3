counter5 = 0
counter3 = 0
counter15 = 0
for n in range(1, 51):
    if n % 5 == 0 and n % 3 == 0:
        print(f'{n} = Five and Three')
        counter15 += 1
    elif n % 5 == 0:
        print(f'{n}=FIVE')
        counter5 += 1
    elif n % 3 == 0:
        print(f'{n}=THREE')
        counter3 += 1
    else:
        print(n)
print(f" \n {counter5} shirheg FIVE, \n {counter3} shirheg THREE, \n {counter15} shirheg FIVE &THREE baina")
